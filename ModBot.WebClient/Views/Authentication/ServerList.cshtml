
<h1>Logged in Discord user: @ViewBag.Test </h1>
@model IEnumerable<GuildModel>
@{
    var AddDiscordBot = "https://discord.com/oauth2/authorize?response_type=code&client_id=844535789882834955&scope=bot%20applications.commands&permissions=2425564350&redirect_uri=https%3A%2F%2Flocalhost%3A5001%2Fauthentication%2Finitializeguild&guild_id=";
}
<div class="p-5 text-center">
    <h1 class="mb-3">Please select server</h1>
</div>
<style>
    #example1 {
        display: flex;
        align-items: center;
        width: 1300px;
        border: 10px;
        padding: 15px;
        position: relative;
        background: #23272a;
        background-clip: border-box;
    }
    img {
        float: left;
        width: 60px;
        height: 60px;
    }

    #Text {
        width: 300px;
        height: 10px;
        float: right;
    }

    #meta {
        width: 1200px;
        display: flex;
        align-items: center;
    }

    #Button {
        position: relative;
        float: right;
        left: 50%;
    }
    #order1 {
        order: 1;
    }
    #order2 {
        order: 2;
    }
</style>
<div id="example1">
    <table>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <div id="meta">
                            @if (item.HasBot == true)
                            {
                                <!-- Do something here to display your items in table format (or whatever format you want with as many columns you want -->
                                @if (item.Icon == null)
                                {
                                    <img src="~/images/Default.png" alt="@item.Name" />
                                }
                                else
                                {
                                    <img src=@item.Icon alt="@item.Name" />
                                }
                                <p id="Text">
                                    @item.Name
                                </p>
                                <div id="Button">
                                    <a asp-area="" asp-controller="Authentication" asp-action="EditGuild" asp-route-guildId="@item.Id" class="btn btn-success">Edit bot</a>
                                </div>
                            }
                            else
                            {
                                @if (item.Icon == null)
                                {
                                    <img src="~/images/Default.png" alt="@item.Name" />
                                }
                                else
                                {
                                    <img src=@item.Icon alt="@item.Name" />
                                }
                                <p id="Text">
                                    @item.Name
                                </p>
                                <div id="Button">
                                    <a href=@AddDiscordBot@item.Id class="btn btn-dark">Add bot</a>
                                </div>
                            }

                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<p></p>